<Window x:Class="FinalProject.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FinalProject"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Name="myGrid">
		<Button Content="Rectangle" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="20,20,20,20" Height="50" Width="100" Click="srDoThing"/>
		<Button Content="Oval" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="150,20,20,20" Height="50" Width="100" Click="srDoThingTwo"/>
		<Button Content="Line" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="300,20,20,20" Height="50" Width="100" Click="srDoThingThree"/>
		<TextBox Name="inputText" Text="Enter Text Here" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="450,20,20,20" Width="100"/>
		<Button Content="Text" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="450,35,20,20" Height="35" Width="100" Click="srDoThingFour"/>
		<Canvas Name="myCanvas" PreviewMouseMove="srOnMouseMove" PreviewMouseUp="srOnMouseUp" Height="450" Width="800"></Canvas>
	</Grid>
</Window>
